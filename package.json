{
  "name": "genemo",
  "version": "1.3.0",
  "description": "Simple to use genetic algorithms library. Supports both NodeJS and web browsers.",
  "keywords": [
    "genetic algorithm",
    "genetic algorithms",
    "evolutionary algorithm",
    "metaheuristics",
    "traveling salesman",
    "traveling salesperson"
  ],
  "main": "lib/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "example1": "node ./examples/example1-findSequence.js",
    "example2": "node ./examples/example2-travelingSalesman.js",
    "example3": "node ./examples/example3-parallelExecution/example3.js",
    "test": "jest --coverage",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .js,.ts",
    "prepush": "npm run lint && npm run test",
    "prepublishOnly": "npm run build && npm run prepush"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  },
  "author": "≈Åukasz Jenczmyk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukix/genemo.git"
  },
  "dependencies": {
    "browser-process-hrtime": "1.0.0",
    "ramda": "0.26.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "coveralls": "3.0.7",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.2",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "random-seed": "0.3.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  }
}
